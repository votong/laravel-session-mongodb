{
    "name": "votong/laravel-session-mongodb",
    "description": "A MongoDB session driver for Laravel 4, 5, 6, 7 and 8",
    "keywords": ["laravel","session","mongodb","mongo","driver"],
    "authors": [
        {
            "name": "Jens Segers",
            "homepage": "http://jenssegers.be"
        }
    ],
    "license" : "MIT",
    "require": {
        "php": ">=8.0",
        "mongodb/laravel-mongodb": "*",
        "illuminate/session": "~4.0|~5.0|~6.0|~7.0|~8.0|~9.0|~10.0|~11.0"
    },
    "autoload": {
        "psr-0": {
            "VoTong\\Mongodb\\Session": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "VoTong\\Mongodb\\Session\\SessionServiceProvider"
            ]
        }
    }
}
